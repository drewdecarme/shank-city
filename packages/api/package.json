{
  "name": "@shank-city/api",
  "version": "0.0.0",
  "private": true,
  "description": "A central api for ShankCity",
  "main": "index.js",
  "type": "module",
  "author": "Drew DeCarme (drewdecarme@gmail.com)",
  "license": "MIT",
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev --port=8282 --remote",
    "db": "dotenv -e ../../.env -- yarn prisma",
    "db:generate": "yarn prisma generate --no-engine",
    "build:types": "tsc --project tsconfig.types.json",
    "build": "yarn db:generate & yarn build:types",
    "test": "yarn flare-city test",
    "lint": "npx eslint ./src/**/* --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drewdecarme/shank-city.git"
  },
  "bugs": {
    "url": "https://github.com/drewdecarme/shank-city/issues"
  },
  "homepage": "https://github.com/drewdecarme/shank-city#readme",
  "dependencies": {
    "@cfworker/jwt": "4.0.6",
    "@flare-city/core": "workspace:^",
    "@flare-city/logger": "workspace:",
    "@flare-city/test": "workspace:^",
    "@prisma/client": "5.4.2",
    "@prisma/extension-accelerate": "0.6.2",
    "prisma": "5.4.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20231016.0",
    "@flare-city/cli": "workspace:^",
    "@flare-city/eslint-config": "workspace:^",
    "@flare-city/tsconfig": "workspace:^",
    "@types/node": "20.8.7",
    "dotenv-cli": "7.3.0",
    "typescript": "5.2.2",
    "wrangler": "3.14.0"
  }
}
